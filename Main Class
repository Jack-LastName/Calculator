import java.util.Scanner;

public class Calc {
	public static void main (String args []) {
		
		try {
		Scanner user = new Scanner(System.in);
		
		/* note Section
		 * Program functions so long as each value and operator are given on separate lines, make it
		 * so they can be added naturally on one line possibly an issue for buffered reader
		 * look into if a catch statement is necessary*/
		
		// Try statement here or some kind of control flow statement to stop leak and control flow
		
		// if char at x = " " read next character FOR allowing natural input and idiot proofing
		// Maybe a switch statement checking each space for an entry vs space
		
		//char at checking not working here as each variable checking unique indexes rather than the line as a whole
		
		System.out.println("Begin Calculation");
		
		int first = user.next().charAt(0);
		char operator = user.next().charAt(2);
		int second = user.next().charAt(4);
		int addition = first+second;
		int minus = first-second;
		int Div = first/second;
		int multi = first*second;
		
		
		switch (operator) {
		case '+':
		{
			System.out.println(addition);
			break;
		}
		case '-':
		{
			System.out.print(minus);
			break;
		}
		case '/':
		{
			System.out.print(Div);
			break;
		}
		case '*':
		{
			System.out.print(multi);
			break;
		}
		default :{
			System.out.print("hi");
			break;
		}
		}
	}finally {
	}
}
}
